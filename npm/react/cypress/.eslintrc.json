{
  "plugins": [
    "cypress"
  ],
  "extends": [
    "plugin:@cypress/dev/react",
    "plugin:@cypress/dev/tests"
  ],
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "mocha/no-global-tests": "off",
    "no-unused-vars": "off",
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ]
  }
}
